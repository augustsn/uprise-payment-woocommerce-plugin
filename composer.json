{
  "config": {
    "platform": {
      "php": "7.3"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "checkout/checkout-sdk-php",
        "version": "2.8.1",
        "type": "library",
        "source": {
          "url": "https://github.com/augustsn/uprise-payment-sdk-php.git",
          "type": "git",
          "reference": "2.8.1"
        },
        "autoload": {
          "psr-4": {
            "Checkout\\": "lib/Checkout"
          }
        }
      }
    }
  ],
  "require": {
    "php": ">=7.3",
    "checkout/checkout-sdk-php": "2.8.1",
    "guzzlehttp/guzzle": "6.5.8",
    "guzzlehttp/promises": "1.5.2",
    "guzzlehttp/psr7": "1.9.0",
    "monolog/monolog": "2.7.0",
    "psr/http-message": "1.0.1",
    "psr/log": "1.1.4",
    "ralouphie/getallheaders": "3.0.3",
    "symfony/polyfill-intl-idn": "v1.26.0",
    "symfony/polyfill-intl-normalizer": "v1.26.0",
    "symfony/polyfill-php72": "v1.26.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6",
    "wp-cli/wp-cli-bundle": "^2.6",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
    "automattic/vipwpcs": "^2.3",
    "phpcompatibility/php-compatibility": "^9.3"
  }
}
